{
   "name": "__MSG_name__",
   "version": "1.0",
   "manifest_version": 2,
   "default_locale": "en",
   "description": "__MSG_description__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "icons": {
       "64": "images/64.png"
   },
   "permissions": [
       "<all_urls>",
       "tabs",
       "history",
       "unlimitedStorage",
       "contextMenus",
       "storage",
       "topSites",
       "activeTab",
       "webRequest",
       "webRequestBlocking"
   ],
   "background":{
      "scripts":[
         "js/background.js"
      ]
   },
   "chrome_url_overrides":{
      "newtab":"pages/newtab.html"
   },
   "browser_action":{
      "default_icon":"images/64.png",
      "default_title": "__MSG_description__"
   },
   "web_accessible_resources":[
      "https://fonts.googleapis.com/icon?family=Material+Icons"
   ]
}